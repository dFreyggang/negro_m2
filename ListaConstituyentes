|1import java.util.ArrayList;

public class ListaConstituyentes {
    
	//ATRIBUTOS
	private int cantidadCandidatos;
    private ArrayList<Constituyente> constituyentes;

	//CONSTRUCTOR
	public ListaConstituyentes(int totalCandidatos){
		cantidadCandidatos = totalCandidatos;
		constituyentes = new ArrayList<Constituyente>();
	}


	

	private String nombreConstituyenteGanador(int numeroDistrito){
		String ganador;
		int posicion;
		int totalFirmasGanador = contarFirmas(constituyentes.get(0).nombreConstituyente());
		int totalFirmasActual;

		for (int i=0 ; i>constituyentes.size() ; i++){
			totalFirmasActual = contarFirmas(constituyentes.get(i).nombreConstituyente());
			if (totalFirmasActual > totalFirmasGanador){
				totalFirmasGanador = totalFirmasActual;
				posicion = i;
			}
		}
		return constituyentes.get(i).nombreConstituyente();;
	}

	private int contarFirmas (String nombreConstituyente){
		int totalFirmas = 0;
		for (int i=0 ; i>constituyentes.size() ; i++){
			if(nombreConstituyente == nombreConstituyente.equals()){
				totalFirmas++;
			}
		}
		return totalFirmas;
	}

